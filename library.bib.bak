@article{Han2010,
abstract = {Cokriging is a statistical interpolation method for the enhanced prediction of a less $\backslash$nintensively sampled primary variable of interest with assistance of intensively sampled $\backslash$nauxiliary variables. In the geostatistics community it is referred to as two- or multi-variable $\backslash$nkriging. In this paper, a new cokriging method is proposed and used for variable-fidelity $\backslash$nsurrogate modeling of aerodynamic data obtained with an expensive high-fidelity CFD code, $\backslash$nassisted by data computed with cheaper lower-fidelity codes or by gradients computed with $\backslash$nan adjoint version of the high-fidelity CFD code, or both. A self-contained derivation as well $\backslash$nas the numerical implementation of this new cokriging method is presented and the $\backslash$ncomparison with the autoregressive model of Kennedy and O'Hagan is discussed. The $\backslash$ndeveloped cokriging method is validated against an analytical problem and applied to $\backslash$nconstruct global approximation models of the aerodynamic coefficients as well as the drag $\backslash$npolar of an RAE 2822 airfoil based on sampled CFD data. The numerical examples show $\backslash$nthat it is efficient, robust and practical for the surrogate modeling of aerodynamic data $\backslash$nbased on a set of CFD methods with varying degrees of fidelity and computational expense. $\backslash$nIt can potentially be applied in the efficient CFD-based aerodynamic analysis and design $\backslash$noptimization of aircraft.},
author = {{Z.-H. Han, R. Zimmermann}, S. G{\"{o}}rtz},
file = {:home/schm{\_}ad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Z.-H. Han, R. Zimmermann - 2010 - A New Cokriging Method for Variable-Fidelity Surrogate Modeling of Aerodynamic Data.pdf:pdf},
isbn = {9781600867392},
journal = {48th AIAA Aerospace Sciences Meeting},
number = {January},
pages = {1--22},
title = {{A New Cokriging Method for Variable-Fidelity Surrogate Modeling of Aerodynamic Data}},
year = {2010}
}
