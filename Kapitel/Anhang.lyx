LatexCommand label
name "sec:Vereinfachung-der-Vorhersageglei"

\end_inset


\end_layout

\begin_layout Standard
An dieser Stelle stellt sich die Frage, inwiefern 
\begin_inset Formula $\mu$
\end_inset

 und der Likelihood Schätzer für 
\begin_inset Formula $\vec{\tilde{\beta}}\in\mathbb{R^{\mathrm{1\times m_{f}}}}$
\end_inset

 zusammenhängen.
 Der Likelihood Schätzer für 
\begin_inset Formula $\vec{\tilde{\beta}}\in\mathbb{R^{\mathrm{1\times m_{f}}}}$
\end_inset

 sieht wie folgt aus: 
\end_layout

\begin_layout Paragraph
\begin_inset Formula 
\[
\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)=\vec{\tilde{\beta}}
\]

\end_inset


\end_layout

\begin_layout Standard
Der Vektor 
\begin_inset Formula $\vec{F}\in\mathbb{R^{\mathrm{n\times1}}}$
\end_inset

 lässt sich folgendermaßen umformulieren, wobei die Definition der Matrix
 
\begin_inset Formula $G\in\mathbb{R^{\mathrm{n\times m_{f}}}}$
\end_inset

 in Gleichung 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:DefinitionG"

\end_inset

 zu finden ist.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\vec{F}=\left[\begin{array}{c}
\vec{\beta_{1}}\\
\vec{\beta_{2}}
\end{array}\right]=G\vec{\tilde{\beta}}=G\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu & =\frac{\left(G\vec{\tilde{\beta}}\right)^{T}\mathbf{Cov}^{-1}\vec{y}}{\left(G\vec{\tilde{\beta}}\right)^{T}\mathbf{Cov}^{-1}G\vec{\tilde{\beta}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu & =\frac{\left(G\vec{\tilde{\beta}}\right)^{T}\mathbf{Cov}^{-1}\vec{y}}{\left(G\vec{\tilde{\beta}}\right)^{T}\mathbf{Cov}^{-1}G\vec{\tilde{\beta}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu & =\frac{\left(G\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)\right)^{T}\mathbf{Cov}^{-1}\vec{y}}{\left(G\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)\right)^{T}\mathbf{Cov}^{-1}G\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu & =\frac{\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)^{T}\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}G^{T}\mathbf{Cov}^{-1}\vec{y}}{\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)^{T}\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}G^{T}\mathbf{Cov}^{-1}G\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu & =\frac{\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)^{T}\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}G^{T}\mathbf{Cov}^{-1}\vec{y}}{\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)^{T}\left(G^{T}\mathbf{Cov}^{-1}G\right)^{-1}\left(G^{T}\mathbf{Cov}^{-1}\overrightarrow{y}\right)}
\end{align*}

\end_inset


\end_layout

