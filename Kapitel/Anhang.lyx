\begin_layout Paragraph*
Behauptung:
\end_layout

\begin_layout Standard
Sei 
\begin_inset Formula $Z\left(X\right)$
\end_inset

 ein schwach stationärer räumlicher Zufallsprozess, so gilt:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
cov\left(\vec{h}\right)=cov\left(\vec{0}\right)-2\gamma\left(\vec{h}\right)\label{eq:DistanzabhängigkeitBehauptung}
\end{equation}

\end_inset

Weiterhin wird benötigt:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
E\left[Z(\vec{0}){}^{2}\right]=cov\left(Z(\vec{0}),Z(\vec{0})\right)-\mu^{2}
\end{equation}

\end_inset

Beweis:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
cov\left(Z(\vec{0}),Z(\vec{0})\right)= & E\left[\left(Z(\vec{0})-E\left[Z(\vec{0})\right]\right)\left(Z(\vec{0})-E\left[Z(\vec{0})\right]\right)\right]\\
= & E\left[Z(\vec{0}){}^{2}-2E\left[Z(\vec{0})\right]Z(\vec{0})+E\left[Z(\vec{0})\right]^{2}\right]\\
= & E\left[Z(\vec{0}){}^{2}-2\mu Z(\vec{0})+\mu^{2}\right]\\
= & E\left[Z(\vec{0}){}^{2}\right]-2\mu^{2}+\mu^{2}\\
cov\left(Z(\vec{0}),Z(\vec{0})\right)-\mu^{2}= & E\left[Z(\vec{0}){}^{2}\right]
\end{align*}

\end_inset

Daraus ergibt sich die notwendige Umformung:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0}+\vec{h})\right)-\mu^{2}= & E\left[Z(\vec{0}+\vec{h})^{2}\right]
\end{align}

\end_inset

Außerdem wird folgende Umformung nötig:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]=cov\left(Z\left(\vec{0}+\vec{h}\right),Z(\vec{0})\right)+\mu^{2}
\end{equation}

\end_inset

Beweis:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0})\right)= & E\left[\left(Z(\vec{0}+\vec{h})-E\left[Z(\vec{0}+\vec{h})\right]\right)\left(Z(\vec{0})-E\left[Z(\vec{0})\right]\right)\right]\\
= & E\left[\left(Z(\vec{0}+\vec{h})-\mu\right)\left(Z(\vec{0})-\mu\right)\right]\\
= & E\left[Z(\vec{0}+\vec{h})Z(\vec{0})-Z(\vec{0}+\vec{h})\mu-\mu Z(\vec{0})+\mu^{2}\right]\\
= & E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]-\mu E\left[Z(\vec{0}+\vec{h})\right]-\mu E\left[Z(\vec{0})\right]+\mu^{2}\\
= & E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]-\mu^{2}\\
cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0})\right)+\mu^{2}= & E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph*
Beweis Gl.
 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:DistanzabhängigkeitBehauptung"

\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
2\gamma\left(\vec{0},\vec{0}+\vec{h}\right) & =\frac{1}{2}var\left[Z(\vec{0}+\vec{h})-Z(\vec{0})\right]\\
 & =\frac{1}{2}E\left[\left(\left(Z(\vec{0}+\vec{h})-Z(\vec{0})\right)-E\left[Z(\vec{0}+\vec{h})-Z(\vec{0})\right]\right)^{2}\right]\\
 & =E\left[\left(Z(\vec{0}+\vec{h})-Z(\vec{0})\right)^{2}-2\left(Z(\vec{0}+\vec{h})-Z(\vec{0})\right)\left(\mu-\mu\right)+\left(\mu-\mu\right)^{2}\right]\\
 & =E\left[\left(Z(\vec{0}+\vec{h})-Z(\vec{0})\right)^{2}\right]\\
 & =E\left[Z(\vec{0}+\vec{h})^{2}-2Z(\vec{0}+\vec{h})Z(\vec{0})+Z(\vec{0}){}^{2}\right]\\
 & =E\left[Z(\vec{0}+\vec{h})^{2}\right]-2E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]+E\left[Z(\vec{0}){}^{2}\right]\\
 & =cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0}+\vec{h})\right)-\mu^{2}-2E\left[Z(\vec{0}+\vec{h})Z(\vec{0})\right]+cov\left(Z(\vec{0}),Z(\vec{0})\right)-\mu^{2}\\
 & =cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0}+\vec{h})\right)-2cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0})\right)+2\mu^{2}+cov\left(Z(\vec{0}),Z(\vec{0})\right)-2\mu^{2}\\
 & =cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0}+\vec{h})\right)-2cov\left(Z(\vec{0}+\vec{h}),Z(\vec{0})\right)+cov\left(Z(\vec{0}),Z(\vec{0})\right)\\
 & =cov\left(\vec{h}\right)-2cov\left(\vec{h}\right)+cov\left(Z(\vec{0}),Z(\vec{0})\right)\\
 & =cov\left(Z(\vec{0}),Z(\vec{0})\right)-cov\left(\vec{h}\right)
\end{align*}

\end_inset


\end_layout

