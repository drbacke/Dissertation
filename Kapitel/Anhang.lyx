LatexCommand label
name "sec:Co-Kriging-KovarianzfunktionHerleitung"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
cov\left(Z_{1}\left(\vec{x}\right),Z_{1}(\vec{x})\right) & =cov\left(aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right),aZ_{2}(\vec{x}^{'})+Z_{diff}(\vec{x}^{'})\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{flalign*}
 & =E\left[\left((aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right))-E[aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right)]\vphantom{\vec{x}^{'}}\right)\right.\\
 & \left.\left((aZ_{2}(\vec{x})+Z_{diff}(\vec{x}^{'}))-E[aZ_{2}(\vec{x}^{'})+Z_{diff}(\vec{x}^{'})]\right)\right]
\end{flalign*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
= & E\left[\left(\left(aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right)\right)-E\left[aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right)\right]\vphantom{\vec{x}^{'}}\right)\right.\\
 & \left.\left(aZ_{2}(\vec{x}^{'})-aE[Z_{1}(\vec{x}^{'})]+Z_{diff}(\vec{x}^{'})-E[Z_{diff}(\vec{x}^{'})]\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
= & E\left[\left((aZ_{2}\left(\vec{x}\right)-aE[Z_{1}\left(\vec{x}\right)])+(Z_{diff}\left(\vec{x}\right)-E\left[Z_{diff}\left(\vec{x}\right)\right])\vphantom{\vec{x}^{'}}\right)\right.\\
 & \left.\left((aZ_{2}(\vec{x}^{'})-aE[Z_{1}(\vec{x}^{'})])+(Z_{diff}(\vec{x}^{'})-E[Z_{diff}(\vec{x}^{'})])\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
= & E\left[a^{2}\left(Z_{2}\left(\vec{x}\right)-E\left[Z_{2}\left(\vec{x}\right)\right]\right)(Z_{2}(\vec{x}^{'})-E[Z_{2}(\vec{x}^{'})])\right]\\
 & +E\left[a\left(Z_{2}\left(\vec{x}\right)-E\left[Z_{2}\left(\vec{x}\right)\right]\right)(Z_{diff}(\vec{x}^{'})-E[Z_{diff}(\vec{x}^{'})])\right]\\
 & +E\left[a\left(Z_{diff}\left(\vec{x}\right)-E\left[Z_{diff}\left(\vec{x}\right)\right]\right)(Z_{2}(\vec{x}^{'})-E[Z_{2}(\vec{x}^{'})])\right]\\
 & +E\left(\left(Z_{diff}\left(\vec{x}\right)-E\left[Z_{diff}\left(\vec{x}\right)\right]\right)(Z_{diff}(\vec{x}^{'})-E[Z_{diff}(\vec{x}^{'})])\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
= & a^{2}cov\left(Z_{2}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)+acov\left(Z_{2}\left(\vec{x}\right),Z_{diff}(\vec{x}^{'})\right)\\
 & +acov\left(Z_{diff}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)+cov\left(Z_{diff}\left(\vec{x}\right),Z_{diff}(\vec{x}^{'})\right)
\end{align*}

\end_inset

Nimmt man ferner an, dass der Differenzprozess 
\begin_inset Formula $Z_{diff}\left(\vec{x}\right)$
\end_inset

 und der Prozess 
\begin_inset Formula $Z_{2}\left(\vec{x}\right)$
\end_inset

 unkorreliert sind, so gilt:
\begin_inset Formula 
\begin{align*}
cov\left(Z_{2}\left(\vec{x}\right),Z_{diff}(\vec{x}^{'})\right) & =0\\
cov\left(Z_{diff}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right) & =0
\end{align*}

\end_inset

daraus folgt:
\begin_inset Formula 
\begin{align*}
cov\left(Z_{1}\left(\vec{x}\right),Z_{1}(\vec{x}^{'})\right)= & a^{2}cov\left(Z_{2}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)+cov\left(Z_{diff}\left(\vec{x}\right),Z_{diff}(\vec{x}^{'})\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\noindent
Modelliert man nun 
\begin_inset Formula $cov\left(Z_{1}\left(\vec{x}\right),Z_{2}\left(\vec{x}^{'}\right)\right)$
\end_inset

 nach demselben Schema, ergibt sich:
\begin_inset Formula 
\begin{align*}
cov\left(Z_{1}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right) & =cov\left(aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
= & E\left[\left(\left(aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right)\right)-E\left[aZ_{2}\left(\vec{x}\right)+Z_{diff}\left(\vec{x}\right)\right]\right)\left(Z_{2}(\vec{x}^{'})-E\left[Z_{2}(\vec{x}^{'})\right]\right)\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
= & E\left[\left(\left(aZ_{2}\left(\vec{x}\right)-E\left[aZ_{2}\left(\vec{x}\right)\right]\right)+\left(Z_{diff}\left(\vec{x}\right)-E\left[Z_{diff}\left(\vec{x}\right)\right]\right)\right)\left(Z_{2}(\vec{x}^{'})-E\left[Z_{2}(\vec{x}^{'})\right]\right)\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
= & E\left[\left(aZ_{2}\left(\vec{x}\right)-E\left[aZ_{2}\left(\vec{x}\right)\right]\right)\left(Z_{2}(\vec{x}^{'})-E\left[Z_{2}(\vec{x}^{'})\right]\right)\right]\\
 & +E\left[\left(Z_{diff}\left(\vec{x}\right)-E\left[Z_{diff}\left(\vec{x}\right)\right]\right)\left(Z_{2}\left(\vec{x}^{'}\right)-E\left[Z_{2}(\vec{x}^{'})\right]\right)\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
= & acov\left(Z_{2}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)+cov\left(Z_{diff}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)
\end{align*}

\end_inset

Es gilt wieder:
\begin_inset Formula 
\begin{align*}
cov\left(Z_{diff}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right) & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\noindent
Daraus ergibt sich:
\begin_inset Formula 
\begin{align*}
cov\left(Z_{1}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)= & acov\left(Z_{2}\left(\vec{x}\right),Z_{2}(\vec{x}^{'})\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
