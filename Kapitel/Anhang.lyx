name "subsec:Exponential-Korrelationsfunktion"

\end_inset


\end_layout

\begin_layout Standard
Die hier gezeigten Ableitungen sind für die Entwicklung der unterschiedlichen
 Kriging-Verfahren nötig und können in anderen Arbeiten dieser Richtung
 hilfreich sein.
 Aus diesem Grund werden die Ergebnisse der Ableitungen hier aufgelistet.
 
\begin_inset Formula 
\begin{align*}
f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right) & =e^{-\frac{1}{2}\overset{k}{\sum}\left(e^{\theta_{k}}\left|x_{1k}-x_{2k}\right|^{p}\right)}+\delta e^{\lambda}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial\theta_{d}} & =f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)\left(-\frac{1}{2}e^{\theta_{k}}\left|x_{1d}-x_{2d}\right|^{p}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial p} & =\left(f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)-\delta e^{\lambda}\right)\left(-\frac{1}{2}\frac{\partial}{\partial p}\left(\overset{k}{\sum}\left(e^{\theta_{k}}\left|x_{1k}-x_{2k}\right|^{p}\right)\right)\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial p} & =\left(f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)-\delta e^{\lambda}\right)\left(-\frac{1}{2}\overset{k}{\sum}e^{\theta_{k}}\frac{\partial}{\partial p}\left(\left|x_{1k}-x_{2k}\right|^{p}\right)\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial p} & =\left(f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)-\delta e^{\lambda}\right)\left(-\frac{1}{2}\overset{k}{\sum}e^{\theta_{k}}\left(\left|x_{1k}-x_{2k}\right|^{p}\right)log\left(\left|x_{1k}-x_{2k}\right|\right)\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial\lambda} & =e^{\lambda}\delta\Rightarrow\frac{\partial R}{\partial\lambda}=e^{\lambda}E
\end{align*}

\end_inset


\end_layout

