\begin_layout Subsection
Herleitung der Kriging-Gewichte
\begin_inset CommandInset label
LatexCommand label
name "sec:Herleitung-der-Kriging-Gewichte"

\end_inset


\end_layout

\begin_layout Standard
Damit ergibt sich:
\begin_inset Formula 
\begin{align*}
\nabla_{w}\left(\textrm{var}\left[Z\left(\vec{x}\right)\right]-2\overrightarrow{\textrm{c}}^{T}\vec{w}+\vec{w}^{T}*\mathbf{Cov}*\vec{w}\right)+\nabla_{w}\left(\lambda\left(\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\beta_{k}\right)\right) & =\vec{0}\\
\wedge\,\,\nabla_{\lambda}\left(\textrm{var}\left[Z\left(\vec{x}\right)\right]-2\overrightarrow{\textrm{c}}^{T}\vec{w}+\vec{w}^{T}*\mathbf{Cov}*\vec{w}\right)+\nabla_{\lambda}\left(\lambda\left(\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\beta_{k}\right)\right) & =0
\end{align*}

\end_inset

Aufgelöst:
\begin_inset Formula 
\begin{align*}
-2\nabla_{w}\left(\overrightarrow{\textrm{c}}^{T}\vec{w}\right)+\nabla_{w}\left(\vec{w}^{T}*\mathbf{Cov}*\vec{w}\right)+\nabla_{w}\lambda\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\nabla_{w}\lambda\beta_{k} & =\vec{0}\\
\wedge\,\,-2\nabla_{\lambda}\left(\overrightarrow{\textrm{c}}^{T}\vec{w}\right)+\nabla_{\lambda}\left(\vec{w}^{T}*\mathbf{Cov}*\vec{w}\right)+\nabla_{\lambda}\lambda\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\nabla_{\lambda}\lambda\beta_{k} & =0
\end{align*}

\end_inset

Zwischenschritt:
\begin_inset Formula 
\begin{align*}
-2\overrightarrow{\textrm{c}}+2\mathbf{Cov}*\vec{w}+\nabla_{w}\lambda\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\nabla_{w}\lambda\beta_{k} & =\vec{0}\\
\wedge\,\,\nabla_{\lambda}\lambda\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\nabla_{\lambda}\lambda\beta_{k} & =0
\end{align*}

\end_inset

Zwischenschritt:
\begin_inset Formula 
\begin{align*}
-2\overrightarrow{\textrm{c}}+2\mathbf{Cov}*\vec{w}+\lambda\nabla_{w}\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j} & =\vec{0}\\
\wedge\,\,\sum_{i=1}^{s}\beta_{i}\sum_{j=1}^{n_{i}}w_{i,j}-\beta_{k} & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Zwischenschritt:
\begin_inset Formula 
\begin{align*}
-2\overrightarrow{\textrm{c}}+2\mathbf{Cov}*\vec{w}+\lambda\vec{F} & =\vec{0}\\
\wedge\,\,\vec{F}^{T}\vec{w} & =\beta_{k}
\end{align*}

\end_inset

Umgeformt:
\begin_inset Formula 
\begin{align*}
\mathbf{Cov}*\vec{w}+\frac{\lambda\vec{F}}{2} & =\vec{c}\\
\wedge\,\,\vec{F}^{T}\vec{w} & =\beta_{k}
\end{align*}

\end_inset

In Matrix Schreibweise:
\begin_inset Formula 
\begin{align*}
\left(\begin{array}{cc}
\mathbf{Cov} & \vec{F}\\
\vec{F}^{T} & 0
\end{array}\right)\left(\begin{array}{c}
\vec{w}\\
\frac{\lambda}{2}
\end{array}\right) & =\left(\begin{array}{c}
\vec{c}\\
\beta_{k}
\end{array}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align}
\Leftrightarrow\left(\begin{array}{c}
\vec{w}\\
\frac{\lambda}{2}
\end{array}\right) & =\left(\begin{array}{cc}
\mathbf{Cov} & \vec{F}\\
\vec{F}^{T} & 0
\end{array}\right)^{-1}\left(\begin{array}{c}
\vec{c}\\
\beta_{k}
\end{array}\right)\label{eq:MatrixKrigingInverse-1-1}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Die Inverse der Blockmatrix ergibt sich nach 
\begin_inset CommandInset citation
LatexCommand citep
key "BlockMatrix"

\end_inset

 zu:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\left(\begin{array}{cc}
\mathbf{Cov} & \vec{F}\\
\vec{F}^{T} & 0
\end{array}\right)^{-1} & =\left(\begin{array}{cc}
\mathbf{Cov}^{-1}-\frac{\mathbf{Cov}^{-1}\vec{F}\vec{F}^{T}\mathbf{Cov}^{-1}}{\vec{F}^{T}\mathbf{Cov}^{-1}\vec{F}} & \frac{\mathbf{Cov}^{-1}\vec{F}}{\vec{F}^{T}\mathbf{R}^{-1}\vec{F}}\\
\frac{\vec{F}^{T}\mathbf{Cov}^{-1}}{\vec{F}^{T}\mathbf{Cov}^{-1}F} & -\frac{1}{\vec{F}^{T}\mathbf{Cov}^{-1}\vec{F}}
\end{array}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Eingesetzt in Gleichung 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:MatrixKrigingInverse-1-1"

\end_inset

:
\begin_inset Formula 
\begin{align*}
\left(\begin{array}{c}
\vec{w}\\
\frac{\lambda}{2}
\end{array}\right) & =\left(\begin{array}{cc}
\mathbf{Cov}^{-1}-\frac{\mathbf{Cov}^{-1}\vec{F}\vec{F}^{T}\mathbf{Cov}^{-1}}{\vec{F}^{T}\mathbf{Cov}^{-1}\vec{F}} & \;\frac{\mathbf{Cov}^{-1}\vec{F}}{\vec{F}^{T}\mathbf{Cov}^{-1}\vec{F}}\\
\frac{\vec{F}^{T}\mathbf{Cov}^{-1}}{\vec{F}^{T}\mathbf{Cov}^{-1}F} & -\frac{1}{\vec{F}^{T}\mathbf{Cov}^{-1}\vec{F}}
\end{array}\right)\left(\begin{array}{c}
\vec{c}\\
\beta_{k}
\end{array}\right)
\end{align*}

\end_inset


