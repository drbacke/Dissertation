\begin_layout Subsection
Gauss Korrelationsfunktion mit Ableitungen
\begin_inset CommandInset label
LatexCommand label
name "subsec:Gauss-Korrelationsfunktion-mit"

\end_inset


\end_layout

\begin_layout Standard
Die hier gezeigten Ableitungen sind für die Entwicklung der unterschiedlichen
 Kriging-Verfahren nötig und können in anderen Arbeiten dieser Richtung
 hilfreich sein.
 Aus diesem Grund werden die Ergebnisse der Ableitungen hier aufgelistet.
 
\begin_inset Formula 
\begin{align*}
f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right) & =e^{-\frac{1}{2}\overset{k}{\sum}\left(e^{\theta_{k}}\left|x_{1k}-x_{2k}\right|^{2}\right)}+\delta e^{\lambda}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
Ableitungen der Hyperparameter
\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial\theta_{d}} & =f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)\left(-\frac{1}{2}e^{\theta_{d}}\left|x_{1d}-x_{2d}\right|^{2}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial\lambda} & =e^{\lambda}\delta\Rightarrow\frac{\partial R}{\partial\lambda}=e^{\lambda}E
\end{align*}

\end_inset

Erste Ableitungen nach dem Ort
\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{1}^{k}} & =\left(f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)-\delta e^{\lambda}\right)\left(-e^{\theta_{k}}\left(x_{1k}-x_{2k}\right)\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{2}^{k}} & =\left(f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)-\delta e^{\lambda}\right)\left(e^{\theta_{k}}\left(x_{1k}-x_{2k}\right)\right)
\end{align*}

\end_inset

Zweite Ableitungen nach dem Ort
\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{1}^{k}\partial x_{2}^{l}} & =\begin{cases}
-e^{\theta_{l}}\left(x_{1l}-x_{2l}\right)\frac{\partial f}{\partial x_{2}^{k}} & k\neq l\\
\left(e^{\theta_{k}}-\left[\left(x_{1k}-x_{2k}\right)e^{\theta_{k}}\right]^{2}\right)f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right) & k=l
\end{cases}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{1}\right)}{\partial x_{1}^{k}\partial x_{1}^{l}} & =\begin{cases}
0 & k\neq l\\
e^{\theta_{k}} & k=l
\end{cases}
\end{align*}

\end_inset

Ableitungen nach Ort und Hyperparameter
\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{1}^{k}\theta_{k}} & =-e^{\theta_{k}}\left(x_{1k}-x_{2k}\right)f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)\left(1-\frac{1}{2}e^{\theta_{k}}\left(x_{1k}-x_{2k}\right)^{2}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{1}^{k}\theta_{p}} & =\frac{1}{2}\left(x_{1k}-x_{2k}\right)f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)\left(e^{\theta_{p}+\theta_{k}}\left(x_{1p}-x_{2p}\right)^{2}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{2}^{k}\theta_{k}} & =f\left(\overrightarrow{x}_{1},\overrightarrow{x}_{2}\right)\left(x_{1k}-x_{2k}\right)e^{\theta_{k}}\left(1-\frac{1}{2}\left(x_{1k}-x_{2k}\right)^{2}e^{\theta_{k}}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
