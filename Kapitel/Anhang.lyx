=\left(a^{2}\left(\sigma_{low}^{2}C_{11A}+\sigma_{err}^{2}C_{11B}\right)-a^{2}C_{12}\left(C_{22}\right)^{-1}\sigma_{low}^{2}C_{12}\right)^{-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\left(a^{2}\left(\sigma_{low}^{2}C_{11A}+\sigma_{err}^{2}C_{11B}-\sigma_{low}^{2}C_{12}\left(C_{22}\right)^{-1}C_{12}\right)\right)^{-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbf{Cov^{-1}}\vec{G}= & \left[\begin{array}{cc}
\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1} & -\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}\\
-C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1} & C_{22}^{-1}+C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}
\end{array}\right]\left[\begin{array}{cc}
\vec{1} & \vec{0}\\
\vec{0} & \vec{1}
\end{array}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbf{Cov^{-1}}\vec{G}= & \left[\begin{array}{cc}
\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}\vec{1} & -\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}\vec{1}\\
-C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}\vec{1} & \left(C_{22}^{-1}+C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}\right)\vec{1}
\end{array}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\vec{G}^{T}\mathbf{Cov^{-1}}\vec{G}= & \left[\begin{array}{cc}
\vec{1}^{T}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}\vec{1} & -\vec{1}^{T}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}\vec{1}\\
-\vec{1}^{T}C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}\vec{1} & \vec{1}^{T}\left(C_{22}^{-1}+C_{22}^{-1}C_{12}\left(C_{11}-C_{12}C_{22}^{-1}C_{12}\right)^{-1}C_{12}C_{22}^{-1}\right)\vec{1}
\end{array}\right]
